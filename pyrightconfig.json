      "reportGeneralTypeIssues": "none",
      "reportAttributeAccessIssue": "none",
      "reportOptionalMemberAccess": "none"
    },
    {
      "root": "src/pillars/geometry/ui",
      "reportGeneralTypeIssues": "none",
      "reportAttributeAccessIssue": "none",
      "reportOptionalMemberAccess": "none"
    },
    {
      "root": "src/pillars/document_manager/ui",
      "reportGeneralTypeIssues": "none",
      "reportAttributeAccessIssue": "none",
      "reportOptionalMemberAccess": "none"
    },
    {
      "root": "src/pillars/tq/ui",
      "reportGeneralTypeIssues": "none",
      "reportAttributeAccessIssue": "none",
      "reportOptionalMemberAccess": "none"
    },
    {
      "root": "src/pillars/adyton/ui",
      "reportGeneralTypeIssues": "none",
