{
  "venvPath": ".",
  "venv": ".venv",
  "include": ["src"],
  "ignore": [
    "scripts/**",
    "tests/**",
    "Docs/**",
    "wiki/**",
    "data/**",
    "saved_documents/**",
    "experiments/**",
    "anamnesis/**",
    "workflow_scripts/**"
  ],
  "exclude": [
    ".venv/**",
    "**/__pycache__/**",
    "**/node_modules/**"
  ],
  "extraPaths": ["src"],
  "typeCheckingMode": "strict",
  "executionEnvironments": [
    {
      "root": "src/pillars/gematria/ui",
      "reportGeneralTypeIssues": "none",
      "reportAttributeAccessIssue": "none",
      "reportOptionalMemberAccess": "none"
    },
